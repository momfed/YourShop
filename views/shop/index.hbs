      <div class="carousel carousel-showmanymoveone slide" id="itemslider">
        <div id="in2" class="carousel-inner">
        
		{{# each offers }}{{#if offer}}
		  <div class="item">
            <div class="col-xs-12 col-sm-6 col-md-2">
              <a href="#"><img src="{{ imagePATH }}" class="img-responsive center-block"></a>
              <span class="badge1">{{ badge }}%</span>
              <h4 class="text-center">{{ title }}</h4>
              <h5 class="text-center">{{ price }} Lek&uml;</h5>
              <h6 class="text-center">{{ price }} Lek&uml;</h6>
            </div>
          </div>{{/if}}
        {{/each}}

        </div>

        <div id="slider-control">
