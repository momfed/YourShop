<p id="AC" data-title="kreu"></p>
<div id="first-slider">
    <div id="carousel-example-generic" class="carousel slide animated bounceInLeft carousel-fade">
        <!-- Indicators -->
       
        <ol class="carousel-indicators"> 
        {{# each offers }}
            	<li data-target="#carousel-example-generic" data-slide-to="{{ slice }}"></li>
        {{/each}}
 		</ol>
              <!-- Wrapper for slides -->
        <div class="carousel-inner" role="listbox">
            <!-- Item 1 -->
            {{# each offers }}
			<div class="item slide1 blur-bgimage" style="background-image: url({{ imagePATH }});background-repeat: no-repeat;background-position: center;background-size: cover;">
                <div class="row"><div class="container" >
                    <div class="col-md-3 text-right">
                        <img style="max-width: 200px;" class="content"  data-animation="animated zoomInLeft" src="{{ imagePATH }}">
                    </div>
                    <div class="col-md-9 text-left">
                        <h3 data-animation="animated bounceInDown text-capitalize">{{ title }}</h3>
                        <h4 data-animation="animated bounceInUp">{{ price }} Lek&euml;</h4>             
                     </div>
                </div></div>
             </div>
             {{/each}}
        </div>
        <!-- End Wrapper for slides-->
        <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
            <i style="display: none;" class="fa commandIco fa-angle-left"></i><span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
            <i style="display: none;" class="fa commandIco fa-angle-right"></i><span class="sr-only">Next</span>
        </a>
    </div>
</div>
<br>


<link href="https://fonts.googleapis.com/css?family=Josefin+Sans:300,400,700&subset=latin-ext" rel="stylesheet">
 

<!--Item slider text-->
