<p id="AC" data-title="marketi"></p>
{{# each products }}
  <div class="row">
  	{{# each this }}
  	  <div class="col-xs-12 col-sm-4 col-md-4">
    	  <div class="thumbnail" style="min-height: 380px">
      		<img src="{{ this.imagePATH }}" alt="..." class="img-responsive">
        	<div class="caption">
          	<h3 class="text-capitalize">{{ this.title }}</h3>
          	<p class="dsc">{{ this.description }}</p>
          	<div class="clearfix">
          		<div class="price pull-left">{{ this.price }} L</div>
          			<a href="#" class="btn btn-success pull-right" role="button">Shto n&euml; shport&euml;</a>
          		</div>
      		</div>
      	</div>
    	</div>
  	{{/each}}
  </div>
{{/each}}
<div align="center">
  <nav id="pagination" data-page="{{ page }}" aria-label="Page navigation">
    <ul class="pagination">
      <li id="fili"><a id="fillimi" href="/market">Fillimi</a></li>
